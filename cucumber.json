{
    "default": {
      "paths": ["src/test/features/"],
      "tags": "${TAGS:-}",
      "formatOptions": {
        "snippetInterface": "async-await" 
      },
      "require": [
        "src/test/stepdefinitions/*.ts", 
        "src/hooks/hooks.ts" 
      ],
      "requireModule": ["ts-node/register"], 
      "format": [
        "html:test-result/report/cucumber-report.html", 
        "json:test-result/report/cucumber-report.json", 
        "rerun:@rerun.txt" 
      ],
      "parallel": 2, 
      "retry": 1 
    },
    "rerun": {
      "paths": ["@rerun.txt"], 
      "tags": "${TAGS:-}", 
      "formatOptions": {
        "snippetInterface": "async-await"
      },
      "require": [
        "src/test/stepdefinitions/*.ts",
        "src/hooks/hooks.ts"
      ],
      "requireModule": ["ts-node/register"],
      "format": [
        "html:test-result/report/rerun-cucumber-report.html",
        "json:test-result/report/rerun-cucumber-report.json",
        "rerun:@rerun.txt"
      ],
      "parallel": 1, 
            "retry": 2
    }
  }
  